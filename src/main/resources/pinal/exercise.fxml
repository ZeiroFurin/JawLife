<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="800.0" prefWidth="1280.0" styleClass="root" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pinal.ExercisePageController">
    <top>
        <HBox alignment="CENTER_LEFT" prefHeight="69.3333" prefWidth="1280.0" styleClass="header-nav">
            <HBox alignment="CENTER_LEFT" spacing="10">
                <Label styleClass="logo-text" text="JawLife" />
            </HBox>
            <Region HBox.hgrow="ALWAYS" />
            <HBox alignment="CENTER" prefHeight="39.0" prefWidth="467.0" spacing="20.0" styleClass="nav-links" stylesheets="@Dashboard.css">
                <Button onAction="#dashboard" text="Overview" />
                <Button styleClass="nav-button-active" text="Exercises" />
                <Button onAction="#handleProgressClick" text="Proggress" />
                <Button onAction="#handleBillingClick" text="Billing" />
                <Button onAction="#handleSettingsClick" text="Settings" />
            </HBox>
            <Region HBox.hgrow="ALWAYS" />
            <HBox alignment="CENTER_RIGHT" spacing="20.0">
                <Button onAction="#handleNotificationsClick" styleClass="icon-button">

                </Button>
                <Button onAction="#handleProfileClick" styleClass="icon-button">
                </Button>
            </HBox>
        </HBox>
    </top>
    <center>
        <ScrollPane fitToWidth="true" hbarPolicy="NEVER" prefWidth="1280.0" style="-fx-background-color: transparent;">
            <content>
                <VBox styleClass="main-content">
                    <VBox alignment="CENTER" styleClass="title-section">
                        <Label styleClass="main-title" text="Today's Exercise" />
                        <Label styleClass="subtitle" text="&quot;The only bad workout is the one that didn't happen.&quot;" />
                    </VBox>
                    <VBox styleClass="category-section">
                        <Label styleClass="category-title" text="Cardio" />
                        <Region styleClass="category-divider" />
                        <FlowPane hgap="20" vgap="20">
                            <Button onAction="#handleRunningClick" styleClass="exercise-card">
                                <graphic>
                                    <HBox alignment="CENTER_LEFT" spacing="10">
                                        <VBox>
                                            <Label styleClass="exercise-name" text="Running" />
                                            <Label styleClass="exercise-details" text="30 minutes" />
                                            <Label styleClass="exercise-details" text="300 calories, 0.9L water lost" />
                                        </VBox>
                                        <Region HBox.hgrow="ALWAYS" />
                                    </HBox>
                                </graphic>
                            </Button>
                            <Button onAction="#handleCyclingClick" styleClass="exercise-card">
                                <graphic>
                                    <HBox alignment="CENTER_LEFT" spacing="10">
                                        <VBox>
                                            <Label styleClass="exercise-name" text="Cycling" />
                                            <Label styleClass="exercise-details" text="20 minutes" />
                                            <Label styleClass="exercise-details" text="200 calories, 0.6L water lost" />
                                        </VBox>
                                        <Region HBox.hgrow="ALWAYS" />
                                    </HBox>
                                </graphic>
                            </Button>
                            <Button onAction="#handleSwimmingClick" styleClass="exercise-card">
                                <graphic>
                                    <HBox alignment="CENTER_LEFT" spacing="10">
                                        <VBox>
                                            <Label styleClass="exercise-name" text="Swimming" />
                                            <Label styleClass="exercise-details" text="25 minutes" />
                                            <Label styleClass="exercise-details" text="250 calories, 0.75L water lost" />
                                        </VBox>
                                        <Region HBox.hgrow="ALWAYS" />
                                    </HBox>
                                </graphic>
                            </Button>
                        </FlowPane>
                    </VBox>
                    <VBox styleClass="category-section">
                        <Label styleClass="category-title" text="Strength Training" />
                        <Region styleClass="category-divider" />
                        <FlowPane hgap="20" vgap="20">
                            <Button onAction="#handlePushupsClick" styleClass="exercise-card">
                                <graphic>
                                    <HBox alignment="CENTER_LEFT" spacing="10">
                                        <VBox>
                                            <Label styleClass="exercise-name" text="Push-ups" />
                                            <Label styleClass="exercise-details" text="5 minutes" />
                                            <Label styleClass="exercise-details" text="50 calories, 0.15L water lost" />
                                        </VBox>
                                        <Region HBox.hgrow="ALWAYS" />
                                    </HBox>
                                </graphic>
                            </Button>
                            <Button onAction="#handleSquatsClick" styleClass="exercise-card">
                                <graphic>
                                    <HBox alignment="CENTER_LEFT" spacing="10">
                                        <VBox>
                                            <Label styleClass="exercise-name" text="Squats" />
                                            <Label styleClass="exercise-details" text="10 minutes" />
                                            <Label styleClass="exercise-details" text="100 calories, 0.3L water lost" />
                                        </VBox>
                                        <Region HBox.hgrow="ALWAYS" />
                                    </HBox>
                                </graphic>
                            </Button>
                            <Button onAction="#handleLungesClick" styleClass="exercise-card">
                                <graphic>
                                    <HBox alignment="CENTER_LEFT" spacing="10">
                                        <VBox>
                                            <Label styleClass="exercise-name" text="Lunges" />
                                            <Label styleClass="exercise-details" text="10 minutes" />
                                            <Label styleClass="exercise-details" text="90 calories, 0.27L water lost" />
                                        </VBox>
                                        <Region HBox.hgrow="ALWAYS" />
                                    </HBox>
                                </graphic>
                            </Button>
                        </FlowPane>
                    </VBox>
                    <VBox styleClass="category-section">
                        <Label styleClass="category-title" text="Flexibility" />
                        <Region styleClass="category-divider" />
                        <FlowPane hgap="20" vgap="20">
                            <Button onAction="#handleYogaClick" styleClass="exercise-card">
                                <graphic>
                                    <HBox alignment="CENTER_LEFT" spacing="10">
                                        <VBox>
                                            <Label styleClass="exercise-name" text="Yoga" />
                                            <Label styleClass="exercise-details" text="15 minutes" />
                                            <Label styleClass="exercise-details" text="50 calories, 0.15L water lost" />
                                        </VBox>
                                        <Region HBox.hgrow="ALWAYS" />
                                    </HBox>
                                </graphic>
                            </Button>
                            <Button onAction="#handleStretchingClick" styleClass="exercise-card">
                                <graphic>
                                    <HBox alignment="CENTER_LEFT" spacing="10">
                                        <VBox>
                                            <Label styleClass="exercise-name" text="Stretching" />
                                            <Label styleClass="exercise-details" text="10 minutes" />
                                            <Label styleClass="exercise-details" text="30 calories, 0.1L water lost" />
                                        </VBox>
                                        <Region HBox.hgrow="ALWAYS" />
                                    </HBox>
                                </graphic>
                            </Button>
                        </FlowPane>
                    </VBox>
               <padding>
                  <Insets top="90.0" />
               </padding>
                </VBox>
            </content>
        </ScrollPane>
    </center>
   <stylesheets>
      <URL value="@exercise.css" />
      <URL value="@global.css" />
      <URL value="@Dashboard.css" />
      <URL value="@billing.css" />
   </stylesheets>
</BorderPane>
